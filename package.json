{
  "name": "fitbot-fullstack",
  "version": "1.0.0",
  "description": "FitBot AI - Full Stack Nutrition Tracking Application",
  "scripts": {
    "dev:all": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm start",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "docker:build": "docker-compose -f docker-compose.fullstack.yml build",
    "docker:up": "docker-compose -f docker-compose.fullstack.yml up",
    "docker:up:build": "docker-compose -f docker-compose.fullstack.yml up --build",
    "docker:down": "docker-compose -f docker-compose.fullstack.yml down"
  },
  "keywords": [
    "fitness",
    "nutrition",
    "chatbot",
    "ai",
    "fullstack"
  ],
  "author": "Jay karan chaturvedi",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
